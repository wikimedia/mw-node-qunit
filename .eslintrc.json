{
	"extends": [
		"wikimedia/server",
		"wikimedia/jquery",
		"wikimedia/qunit"
	],
	"env": {
		"browser": true
	},
	"globals": {
		"mediaWiki": "readonly"
	},
	"rules": {
		"no-var": "error"
	},
	"settings": {
		"jsdoc": {
			"preferredTypes": {
				"sinon": "sinon",
				"NodeJS": "NodeJS"
			}
		}
	}
}
