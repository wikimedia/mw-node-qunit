	"license": "GPL-3.0",
	"bugs": "https://github.com/wikimedia/mw-node-qunit/issues",
	"homepage": "https://github.com/wikimedia/mw-node-qunit#readme",
	"dependencies": {
		"eslint-config-wikimedia": "0.20.0",
		"glob": "7.1.7",
		"jquery": "3.6.0",
		"jsdom": "16.6.0",
		"mustache": "4.2.0",
		"oojs": "6.0.0",
		"prettier": "2.3.1",
		"qunit": "2.16.0",
		"sinon": "11.1.1"
